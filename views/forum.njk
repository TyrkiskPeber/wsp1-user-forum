{% extends "layout.njk" %}
{% block content %}

    <ul>
    {% for rows in rows%}
        <li class="forum">
            <h1>{{rows.title | safe }}</h1>
            <p>{{rows.content}}</p>
            <p>Posted at {{ rows.createdAt }} by <b>{{ rows.name }}</b></p>
        </li>
    {%endfor%}
    </ul>
{% endblock %}